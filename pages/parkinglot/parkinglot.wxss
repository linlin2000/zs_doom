.parkinglot {
  position: fixed;
  width: 100%;
}
i-tab {
  width: 50%;
  height: 88rpx;
  z-index: 999;
}
i-tab .i-tabs-tab-title {
  font-size: 30rpx;
  color: #5c8fff;
  z-index: 999;
}
.pick-container {
  width: 100%;
  height: 600rpx;
  padding-top: 88rpx;
  box-sizing: border-box;
  background-color: #fff;
  position: absolute;
  top: -600rpx;
}
.pick_lin {
  border-bottom: 1rpx solid #ccc;
  font-size: 28rpx;
  text-align: center;
  height: 66rpx;
  line-height: 66rpx;
}
.pick_lin.link {
  color: #5c8fff;
}
@keyframes slidown {
  from {
    transform: translateY(0%);
  }
  to {
    transform: translateY(100%);
  }
}
.slidown {
  display: block;
  animation: slidown 0.1s ease-in-out both;
}
@keyframes slidup {
  from {
    transform: translateY(100%);
  }
  to {
    transform: translateY(0%);
  }
}
.slidup {
  display: block;
  animation: slidup 0.3s ease-in-out both;
}
.shadow {
  width: 100%;
  background-color: rgba(1, 1, 1, 0.2);
  position: absolute;
  z-index: 40;
  top: 88rpx;
}
.contact {
  background-color: #ccc;
  padding-top: 88rpx;
}
.contact .centre {
  padding: 20rpx;
  background-color: #fff;
}
.contact .centre .centre_ge {
  height: 149rpx;
  border-bottom: 1rpx solid #ccc;
  box-sizing: border-box;
}
.contact .centre .centre_ge .centre_le {
  float: left;
  width: 80%;
}
.contact .centre .centre_ge .centre_le .centre_top {
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
  margin-top: 10rpx;
  font-size: 38rpx;
  line-height: 70rpx;
  color: #333333;
}
.contact .centre .centre_ge .centre_le .centre_bott {
  overflow: hidden;
text-overflow:ellipsis;
white-space: nowrap;
  font-size: 24rpx;
  line-height: 60rpx;
  color: #666666;
}
.contact .centre .centre_ge .centre_re {
  height: 100%;
  padding: 18rpx 30rpx;
  box-sizing: border-box;
  font-size: 20rpx;
  float: right;
  color: #ccc;
  text-align: center;
}
.contact .centre .centre_ge .centre_re image {
  display: block;
  height: 60rpx;
  width: 60rpx;
  margin-bottom: 15rpx;
}